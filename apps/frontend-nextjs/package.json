{
  "name": "frontend-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "test": "yarn lint-staged",
    "start": "next start",
    "lint": "next lint",
    "prepare": "cd ../../ && husky apps/frontend-nextjs/.husky",
    "postinstall": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --list-different --write",
      "eslint --max-warnings 0",
      "bash -c 'yarn run type-check'"
    ]
  },
  "dependencies": {
    "@navikt/ds-tailwind": "^7.24.0",
    "next": "15.3.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/node": "^24.0.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "eslint": "^9.29.0",
    "eslint-config-next": "15.3.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "typescript": "^5"
  }
}
