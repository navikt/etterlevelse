    "@fortawesome/react-fontawesome": "3.1.1",
    "@navikt/aksel-icons": "8.0.1",
    "@navikt/ds-css": "8.0.1",
    "@navikt/ds-react": "8.0.1",
    "@navikt/ds-tailwind": "8.0.1",
    "@tailwindcss/postcss": "4.1.18",
    "amplitude-js": "8.21.9",
    "axios": "1.13.2",
    "draft-js": "0.11.7",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.5",
