{
  "name": "frontend-nextjs",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "test": "yarn lint-staged",
    "start": "next start",
    "lint": "next . --ignore-path ../../.eslintignore && prettier -c . --ignore-path ../../.prettierignore lint",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write \"./app\"",
    "prepare": "cd ../../ && husky apps/frontend-nextjs/.husky",
    "prettier": "prettier --write \"./app\"",
    "type-check": "tsc --project tsconfig.json",
    "postinstall": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --list-different --write",
      "eslint --max-warnings 0",
      "bash -c 'yarn run type-check'"
    ]
  },
  "eslintConfig": {
    "extends": "next",
    "rules": {
      "semi": [
        "warn",
        "never"
      ],
      "quotes": [
        "warn",
        "single"
      ],
      "react-hooks/exhaustive-deps": "off"
    }
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@fontsource/source-sans-pro": "^5.2.5",
    "@fortawesome/fontawesome-svg-core": "^7.0.0",
    "@fortawesome/free-brands-svg-icons": "^7.0.0",
    "@fortawesome/free-solid-svg-icons": "^7.0.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@navikt/aksel-icons": "^7.25.1",
    "@navikt/ds-css": "^7.25.1",
    "@navikt/ds-react": "^7.25.1",
    "@navikt/ds-tailwind": "^7.25.1",
    "@tailwindcss/postcss": "^4.1.11",
    "amplitude-js": "^8.21.9",
    "axios": "^1.10.0",
    "draft-js": "^0.11.7",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "graphql": "^16.11.0",
    "markdown-draft-js": "^2.4.0",
    "moment": "^2.30.1",
    "next": "15.4.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-helmet-async": "^2.0.5",
    "react-markdown": "^10.1.0",
    "react-markdown-editor-lite": "^1.3.4",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "tailwindcss": "^4.1.11",
    "yup": "^1.6.1",
    "lodash": "^4.17.21",
    "react-json-view-lite": "2.4.1"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/amplitude-js": "^8.16.5",
    "@types/draft-js": "^0.11.18",
    "@types/lodash": "^4.17.20",
    "@types/markdown-draft-js": "^2.2.7",
    "@types/node": "^24.0.15",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-draft-wysiwyg": "^1.13.8",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.31.0",
    "eslint-config-next": "15.4.2",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "formik": "^2.4.6",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "lodash": "^4.17.21",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "react-draft-wysiwyg": "^1.15.0",
    "react-select": "^5.10.1",
    "typescript": "^5.8.3",
    "@types/lodash": "^4.17.20"
  }
}
