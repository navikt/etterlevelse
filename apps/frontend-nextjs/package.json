    "moment": "2.30.1",
    "next": "16.1.4",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-helmet-async": "2.0.5",
    "react-json-view-lite": "2.5.0",
    "react-markdown": "10.1.0",
    "react-markdown-editor-lite": "1.4.1",
    "react-process-string": "1.2.0",
    "rehype-raw": "7.0.0",
