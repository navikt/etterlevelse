scalar DateTime
scalar Date
scalar NonNegativeInt

# Queries

type Query {
    kravById(id: ID!): Krav
    kravByNummer(nummer: Int!, versjon: Int!): Krav
    krav(filter: KravFilter, pageNumber: NonNegativeInt = 0, pageSize: NonNegativeInt = 0): [Krav!]!

    etterlevelseById(id: ID!) : Etterlevelse
}

input KravFilter {
    relevans: [String!]
    nummer: Int
}