scalar DateTime
scalar Date
scalar PositiveInt

# Queries

type Query {
    kravById(id: ID!): Krav
    kravByNummer(nummer: Int!, versjon: Int!): Krav
    krav(filter: KravFilter, first: PositiveInt = 0, skip: PositiveInt = 0): [Krav!]!

    etterlevelseById(id: ID!) : Etterlevelse
}

input KravFilter {
    relevans: String
    nummer: Int
}