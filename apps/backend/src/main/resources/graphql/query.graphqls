scalar DateTime
scalar Date
scalar NonNegativeInt

# Queries

type Query {
    kravById(id: ID!): Krav
    kravByNummer(nummer: Int!, versjon: Int!): Krav
    krav(filter: KravFilter, skip: NonNegativeInt = 0, first: NonNegativeInt = 0): [Krav!]!

    etterlevelseById(id: ID!) : Etterlevelse
}

input KravFilter {
    relevans: String
    nummer: Int
}